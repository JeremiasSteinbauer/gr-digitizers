<?xml version="1.0"?>
<block>
  <name>B.2 Demux</name>
  <key>digitizers_extractor</key>
  <category>[digitizers]</category>
  <import>import digitizers</import>
  <make>digitizers.extractor($post_trigger_window, $pre_trigger_window)
  </make>
 
  <param>
    <name>Pre-trigger Window (s)</name>
    <key>pre_trigger_window</key>
    <value>0.0</value>
    <type>float</type>
  </param>
  <param>
    <name>Post-trigger Window (s)</name>
    <key>post_trigger_window</key>
    <value>0.001</value>
    <type>float</type>
  </param>
  
  <sink>
    <name>values</name>
    <type>float</type>
  </sink>
  <sink>
    <name>errors</name>
    <type>float</type>
    <optional>True</optional>
  </sink>

  <source>
    <name>values</name>
    <type>float</type>
  </source>
  <source>
    <name>errors</name>
    <type>float</type>
    <optional>True</optional>
  </source>
</block>
